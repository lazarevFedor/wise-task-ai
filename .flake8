[flake8]
max-line-length = 90
max-complexity = 10
ignore =
    W503,
    F811,
    D100, D101, D102, D103, D104, D105, D106, D107,
    D203, D205, D213,
    D400, D401,
    N806, N818,
    E402
exclude =
    .venv/
    venv/
    env/
    proto/
    docker/
    server/
    third_party/
    llm-service/prompts/
    llm-service/app/llm_service/
